/*to find the greatest of 2 numbers*/
#include<stdio.h>
#include<conio.h>
void main()
{
    int a,b;
    clrscr();
    printf("enter 2 numbers \n");
    scanf("%d%d",&a,&b);
    if(a>b)
      printf("a=%d is greater than b=%d\n",a,b);
    else if(b>a)
      printf("b=%d is greater than a=%d\n",b,a);
    else
      printf("a is equal to b\n");
    getch();
    getch();
}


/*to convert farenheit to celsius temperature*/
#include<stdio.h>
#include<conio.h>
void main()
{
    float fare,cels;
    clrscr();
    printf("Enter the temperature in farenheit \n");
    scanf("%f",&fare);
    cels=((fare-32.0)*5)/9;
    printf("the temperature in celsius %f",cels);
    getch();
}

